{
  "name": "befuturein/core-shared",
  "description": "Shared core layer for BeFuture applications.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "BeFuture\\CoreShared\\": "src/"
    },
    "files": ["src/Support/helpers.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "BeFuture\\CoreShared\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "BeFuture\\CoreShared\\CoreSharedServiceProvider"
      ]
    }
  }
}
